get_small_struct: fn(val: i32) -> SmallStruct = c_import!("get_small_struct");
get_int: fn(val: i32) -> i32 = c_import!("get_int");
main: fn -> i32 = {
	bs := get_big_struct(10);
	ssa := get_small_struct(10);
	int := get_int(10);

	test::print(&"int: ${int}");
	test::print(&"small struct: ${ss.a}, ${ss.b}");
	test::print(&"big struct: ${bs.a}, ${bs.b}, ${bs.c}, ${bs.d}");
