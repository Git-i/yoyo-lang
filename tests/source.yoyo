app: module = APP
always_true: fn -> bool = {
    "True function called".app::func();
    return true;
}
always_false: fn -> bool = {
    "False function called".app::func();
    return false;
}
main: fn = {
    val: mut = false;
    val = always_true() && always_true();
    "${val}----------------".app::func();
    val = always_false() && always_true();
    "${val}----------------".app::func();
    val = always_true() && always_false();
    "${val}----------------".app::func();
    "--------OR---------------".app::func();
    val = always_true() || always_true();
    "${val}----------------".app::func();
    val = always_false() || always_true();
    "${val}----------------".app::func();
    val = always_true() || always_false();
    "${val}----------------".app::func();
}